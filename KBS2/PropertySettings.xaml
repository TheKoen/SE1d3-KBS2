<UserControl x:Class="KBS2.PropertySettings"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:KBS2"
             mc:Ignorable="d" 
             d:DesignHeight="30" d:DesignWidth="450">
    <Grid Margin="0,0,-0.4,0.4" Background="#FF666666">
        <Label x:Name="LabelPropertyName" Content="PropertyName" HorizontalAlignment="Left" VerticalAlignment="Top" Height="29" Width="163" Background="#FF666666" BorderBrush="#FF666666" Foreground="#FF00FFD6"/>
        <TextBox x:Name="TBCurrentValue" Text="CurrentValue" HorizontalAlignment="Left" Margin="163,-1,0,0" VerticalAlignment="Top" Width="269" Height="31" Background="#FF666666" BorderBrush="#FF666666" Foreground="#FF00FFD6"/>
    </Grid>
</UserControl>
